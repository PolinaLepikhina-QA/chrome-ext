{
  "manifest_version": 3,
  "name": "Hello World",
  "description": "First EXT",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content-script-loader.index.tsx-f33ced33-37452ee4.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/index-99a12d73.js",
        "assets/index.tsx-f33ced33.js"
      ],
      "use_dynamic_url": true
    }
  ]
}